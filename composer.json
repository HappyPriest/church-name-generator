{
    "name": "happypriest/church-name-generator",
    "description": "Fake Church Name Generator",
    "keywords": [
        "happypriest",
        "church-name-generator",
        "fake-church-name-generator",
        "faker"
    ],
    "homepage": "https://github.com/happypriest/church-name-generator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Daniel Felix",
            "email": "danielfelix1995@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "fzaninotto/faker": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "HappyPriest\\ChurchNameGenerator\\": "src"
        }
    },
    "autoload-dev": {
